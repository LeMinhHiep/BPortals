@using MvcReportViewer;
@model MVCClient.ViewModels.Helpers.PrintViewModel

@if (@Model.PrintOptionID == 1 || @Model.PrintOptionID == 3)
{
    @Html.MvcReportViewerFluent("/TotalBikePortals/VehicleTransferDetailSheet").ReportParameters(new { SwitchSQLID = 2, StockTransferID = @Model.Id }).Attributes(new { Height = 1100, Width = 1150, style = "border: none" }).ControlSettings(new ControlSettings() { ShowParameterPrompts = false });
    @Html.MvcReportViewerFluent("/TotalBikePortals/VehicleTransferSheet").ReportParameters(new { SwitchSQLID = 2, StockTransferID = @Model.Id }).Attributes(new { Height = 1100, Width = 1150, style = "border: none" }).ControlSettings(new ControlSettings() { ShowParameterPrompts = false });
}

@if (@Model.PrintOptionID == 2 || @Model.PrintOptionID == 3)
{
    @Html.MvcReportViewerFluent("/TotalBikePortals/PartTransferDetailSheet").ReportParameters(new { SwitchSQLID = 2, StockTransferID = @Model.Id }).Attributes(new { Height = 1100, Width = 1150, style = "border: none" }).ControlSettings(new ControlSettings() { ShowParameterPrompts = false });
    @Html.MvcReportViewerFluent("/TotalBikePortals/PartTransferSheet").ReportParameters(new { SwitchSQLID = 2, StockTransferID = @Model.Id }).Attributes(new { Height = 1100, Width = 1150, style = "border: none" }).ControlSettings(new ControlSettings() { ShowParameterPrompts = false });
}